}

QTableWidget::item:selected {
    background: #0D5277;
    color: #EAE9FC;
	font-size: 22px;
    border-radius: 0px;
    border: 1px solid white;
}

QTableWidget QLineEdit {
	background-color: rgb(25, 26, 30);
	border-radius: none;
}


QTableView::item:selected {
    border: none;
}
QAbstractItem::item:selected {
    border: none;
}

