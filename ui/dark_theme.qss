QSpinBox {
    border-radius: 5px;
    background-color: rgba(15, 15 15, 130);
    padding: 5px 10px;
    color: #EAE9FC;
    border: 1px solid rgba(65, 65, 65, 0.4);
}

QSpinBox:hover {
    border-color: #3daee9;
}

QSpinBox:focus {
    border-color: #3daee9;
}

QSpinBox:disabled {
    color: rgba(144, 144, 144, 130)
}

QDoubleSpinBox {
    border-radius: 5px;
    background-color: rgba(15, 15, 15, 0.6);
    padding: 5px 5px;
    color: #EAE9FC;
    border: 1px solid rgb(65, 65, 65);
}

QDoubleSpinBox:hover {
    border-color: #3daee9;
}

QDoubleSpinBox:focus {
    border-color: #3daee9;
}

QDoubleSpinBox:disabled {
    color: #8C8C8C;
}

QAbstractSpinBox:up-button,
QAbstractSpinBox:up-button:hover {
    background-color: transparent;
    subcontrol-origin: padding;
    subcontrol-position: top right;
    padding-right: 0.1em;
    padding-top: 0.1em;
    width: 0.6em;
    height: 0.5em;
}

QAbstractSpinBox:down-button,
QAbstractSpinBox:down-button:hover {
    background-color: transparent;
    subcontrol-origin: padding;
    subcontrol-position: bottom right;
    padding-right: 0.1em;
    padding-bottom: 0.1em;
    width: 0.6em;
    height: 0.5em;
}

QAbstractSpinBox::up-arrow,
QAbstractSpinBox::up-arrow:disabled,
QAbstractSpinBox::up-arrow:off,
QAbstractSpinBox::up-arrow:!off:!disabled:hover,
QAbstractSpinBox::down-arrow,
QAbstractSpinBox::down-arrow:disabled,
QAbstractSpinBox::down-arrow:off,
QAbstractSpinBox::down-arrow:!off:!disabled:hover {
    border-image: none;
    width: 0.6em;
    height: 0.5em;
}

QAbstractSpinBox::up-arrow {
    image: url(ui/svg/up_arrow.svg);
}

QAbstractSpinBox::up-arrow:disabled,
QAbstractSpinBox::up-arrow:off {
    image: url(ui/svg/up_arrow_disabled.svg);
}

QAbstractSpinBox::up-arrow:hover {
    image: url(ui/svg/up_arrow_hover.svg);
}

QAbstractSpinBox::down-arrow {
    image: url(ui/svg/down_arrow.svg);
}

QAbstractSpinBox::down-arrow:disabled,
QAbstractSpinBox::down-arrow:off {
    image: url(ui/svg/down_arrow_disabled.svg);
}

QAbstractSpinBox::down-arrow:!off:!disabled:hover {
    image: url(ui/svg/down_arrow_hover.svg);
}

QDateEdit {
    background-color: rgba(15, 15, 15, 0.6);
    border: 1px solid rgb(65, 65, 65);
    border-radius: 5px;
    padding: 5px 10px;
    color: #EAE9FC;
}

QDateEdit::editable {
    background-color: rgba(15, 15, 15, 0.6);
}

QComboBox::editable:focus {
    border-color: #3daee9;
}

QDateEdit:hover {
    border-color: #3daee9;
}

QDateEdit:disabled {
    color: rgb(102, 102, 102);
}

QDateEdit::drop-down:button {
    border-radius: 2px;
}

QDateEdit::down-arrow {
    image: url(ui/svg/drop-down.svg);
    width: 12px;
    height: 12px;
}

QLabel {
    background-color: none;
    border: none;
    padding: 2px 5px;
    color: #EAE9FC;
}
/* https://www.qtcentre.org/threads/30478-How-To-Change-Style-Sheet-for-QCalendarWidget */
QCalendarWidget QAbstractItemView:item{
    border: 1px solid #151515;
}
QCalendarWidget QWidget{
	background-color: #151515;
	border-radius: 5px;
	margin-bottom: 8px;
}
QCalendarWidget QAbstractItemView:item:selected {
    background-color: #3daee9;
	color: #171717;
}
QCalendarWidget QAbstractItemView:item:disabled {
	color: rgba(102, 102, 102, 0.6);
    background-color: rgba(44, 44, 44, 0.6);
}
QCalendarWidget QAbstractItemView:item:hover {
    border-color: #3daee9;
}
QCalendarWidget QAbstractItemView:item:focus {
    background-color: #3daee9;
    border-color: #3daee9;
	color: #171717;
}

QCalendarWidget QToolButton#qt_calendar_prevmonth {
    qproperty-icon: url(ui/svg/calendar_previous.svg);
}

QCalendarWidget QToolButton#qt_calendar_nextmonth {
    qproperty-icon: url(ui/svg/calendar_next.svg);
}

QCalendarWidget QAbstractItemView:disabled { color: rgb(64, 64, 64); }
/* days in other months */
QCalendarWidget QAbstractItemView:enabled
{
	color: rgb(180, 180, 180);
	selection-background-color: rgb(64, 64, 64);
	selection-color: rgb(0, 255, 0);
}
