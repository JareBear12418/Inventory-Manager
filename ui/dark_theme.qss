/* Main Window */

QMainWindow::separator {
    background: #1e1e1e;
    width: 4px;
}

QMenuBar {
    background-color: #181818;
}

QMenuBar::item {
    padding: 5px;
    background-color: #181818;
    border: 1px solid #181818;
}

QMenuBar::item:selected {
    border-color: #3daee9;
    color: #EAE9FC;
}

QMenuBar::item:pressed {
    background-color: #3daee9;
    color: #171717;
}
QMenuBar:disabled {
    color: #666666;
}
QMenu::item{
    padding: 5px;
    background-color: #303030;
    border: 1px solid #303030;
}
QMenu::item:selected{
    background-color: #303030;
    border-color: #3daee9;
    color: #EAE9FC;
}
QMenu::item:selected:pressed{
    background-color: #3daee9;
    color: #171717;
}
QMenu::item:checked{
    background-color: #3daee9;
    border-color: #3daee9;
    color: #171717;
}
QMenu::separator {
    background-color: #86898a;
}

QStatusBar {
    background-color: #242424;
    border: 1px solid #242424;
}

/* Containers */

QDialog {
    /* background-color: #181818; */
}


QWidget {

    border: none;
}
QGridWidget{
    border-radius: 5px;
}
QGroupBox {
    background-color: rgba(20, 20, 20, 0.5);
    border: 1px solid rgba(15, 15, 15, 0.7);
    border-radius: 10px;
    font-size: 14px;
    margin-top: 1em;
    padding-top: 0.6em;
}
QGroupBox::title {
    top: -1.4em;
    color: #EAE9FC;
    subcontrol-origin: content;
    subcontrol-position: top center;
    padding-left: 0.2em;
    padding-right: 0.2em;
}

QScrollArea {
    background: transparent;
    width: 10px;
    border-radius: 0px;
    margin: 0px;
}

QDockWidget {
    color: #8C8C8C;
}


QDockWidget::title {
    background: rgb(24, 24, 24);
}

